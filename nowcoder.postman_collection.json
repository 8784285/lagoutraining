		{
			"name": "3.2 case2：验证签名成功_script3.2-2",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "5e9185f6-1b8b-4ad6-9cc7-abc591665f07",
						"exec": [
							"// 定义变量值",
							"p = \"open\"",
							"username = \"Mr.null\"",
							"password = \"dc483e80a7a0bd9ef71d8cf973673924\"",
							"",
							"",
							"//设置当前时间戳",
							"postman.setGlobalVariable(\"time\",Math.round(new Date().getTime()));",
							"time = postman.getGlobalVariable('time')",
							"",
							"//设置KEY_WORD为全局变量",
							"postman.setGlobalVariable(\"Key\",\"a7a0bd9ef71d8cf9\")",
							"KEY_WORD = postman.getGlobalVariable(\"Key\");",
							"",
							"",
							"//字符串进行md5加密",
							"var str = p+username+password+time+KEY_WORD;",
							"var strmd5= CryptoJS.MD5(str).toString();",
							"postman.setGlobalVariable(\"sign\",strmd5)",
							""
						],
						"type": "text/javascript"
					}
				}
			],
